{% extends "base.twig" %}

{% block content %}

	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1">{{post.post_title}}</h1>
				
				
				{{post.post_content|raw|wpautop}}
			</section>

		</article>
	</div> <!-- /content-wrapper -->

{% endblock %}